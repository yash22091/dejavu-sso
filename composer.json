    "type": "project",
    "require": {
        "php": ">=7.0,<8.0",
        "ext-mysqli": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "jumbojett/openid-connect-php": "^0.9.0",
